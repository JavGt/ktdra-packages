{
	"compilerOptions": {
		"target": "ES2016",
		"module": "ES6",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"declaration": true,
		"allowJs": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"moduleResolution": "node",
		"jsx": "react",
		"sourceMap": true
	},
	"exclude": ["node_modules"]
}
